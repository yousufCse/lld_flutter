    "version": "0.2.0",
    "configurations": [
        {
            "name": "prod",
            "request": "launch",
            "type": "dart",
            "args": [
                "--flavor",
                "prod",
                "--dart-define-from-file=env/prod.json"
            ]
        },
        {
            "name": "dev",
            "request": "launch",
            "type": "dart",
            "args": [
                "--flavor",
                "dev",
                "--dart-define-from-file=env/dev.json"
            ]
        },
        {
            "name": "lld_flutter (debug mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug"
        },
        {
            "name": "lld_flutter (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "lld_flutter (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}